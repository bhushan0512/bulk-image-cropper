# Ignore all files in the output directory but not the directory itself
/output/*
! /output/.gitkeep

# Ignore all files in the images directory but not the directory itself
/images/*
! /images/.gitkeep

# Ignore all files in the venv directory but not the directory itself
/venv/*
! /venv/.gitkeep

# Ignore Python bytecode files
*.pyc
*.pyo
*.pyd
__pycache__/

# Ignore environment files
.env

# Ignore IDE/editor configuration files
.vscode/
.idea/

# Ignore system files
.DS_Store
Thumbs.db

# Ignore log files
*.log
